    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "core-js": "^3.15.1",
    "cssnano": "^5.0.6",
    "gulp": "^4.0.2",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
