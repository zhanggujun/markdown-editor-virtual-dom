{
  "private": false,
  "name": "edit.md",
  "version": "0.0.1",
  "description": "基于snabbdom、markdown-it、marked、highlight、github-markdown.css的简单markdown编辑器",
  "main": "core/index.js",
  "style": "core/index.css",
  "bugs": {
    "url": "https://github.com/zhanggujun/markdown-editor-virtual-dom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhanggujun/markdown-editor-virtual-dom"
  },
  "scripts": {
    "clear": "rm -rf core",
    "dev": "npm run clear && cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm run clear && cross-env NODE_ENV=production webpack",
    "publish": "npm publish --access=public --registry=https://registry.npmjs.org/"
  },
  "keywords": [
    "snabbdom、markdown-it、marked、highlight、github-markdown.css、markdown"
  ],
  "author": "zhanggujun",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime-corejs3": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "core/*",
    "README.md"
  ],
  "dependencies": {
    "lodash": "^4.17.20",
    "snabbdom": "^2.1.0"
